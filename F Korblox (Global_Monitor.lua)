local p = game.Players.LocalPlayer
local function run(c)
    local h = c:WaitForChild("Head", 5)
    if h then h.Transparency = 1 for _, v in pairs(h:GetChildren()) do if v:IsA("Decal") then v.Transparency = 1 end end end
    task.wait(1)
    pcall(function() loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-Korblox-All-61127"))() end)
end

task.spawn(function()
    print("//// #### [M3] BOOTING_MONITOR_SERVICE #### ////")
    if p.Character then task.spawn(run, p.Character) end
    p.CharacterAdded:Connect(function(char) run(char) end)
    local function monitorOther(player) player.CharacterAdded:Connect(function(char) run(char) end) end
    for _, player in pairs(game.Players:GetPlayers()) do if player ~= p then monitorOther(player) end end
    game.Players.PlayerAdded:Connect(monitorOther)
end)
